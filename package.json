{
  "name": "svelte-app",
  "version": "1.0.0",
  "description": "A Svelte application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "cap:add": "capacitor add",
    "cap:sync": "npm run build && npx cap sync",
    "cap:copy": "npm run build && npx cap copy",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:electron": "npx cap open electron",
    "cap:run:android": "npm run cap:sync && npx cap run android",
    "cap:run:ios": "npm run cap:sync && npx cap run ios",
    "cap:run:electron": "npm run cap:sync && npx cap run electron",
    "electron:dev": "npm run build && npx cap sync electron && npx cap open electron",
    "electron:build": "npm run build && npx cap sync electron",
    "electron:start": "cd electron && npm run build && npm run electron:start",
    "electron:start-live": "cd electron && npm run electron:start-live"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1",
    "svelte-spa-router": "^4.0.1",
    "y-websocket": "^1.5.4",
    "yjs": "^13.6.28"
  },
  "devDependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/svelte": "^4.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "jsdom": "^23.0.0",
    "svelte": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "svelte",
    "vite",
    "typescript"
  ]
}
